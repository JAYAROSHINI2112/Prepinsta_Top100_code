
#include <stdio.h>

int main()
{
    int n,a[100],i,j,max=0,m = 0;
    scanf("%d",&n);
    for(i = 0; i < n; i++)
    scanf("%d",&a[i]);
    for(i = 0; i < n; i++)
    {
        if(max < a[i])
         max = a[i];
    }
    for(i = 0; i < n; i++)
    {
        if(a[i] > m && a[i] < max)
          m = a[i];
    }
    printf("%d",m);
}

